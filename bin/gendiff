#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';


$doc = <<<DOC
Generate diff

Usage:
  gendiff (-h|--help)
  gendiff (-v|--version)

Options:
  -h --help                     Show this screen
  -v --version                  Show version
DOC;

$args = Docopt::handle($doc, array('version' => 'Some version let it be 1.1.1'));